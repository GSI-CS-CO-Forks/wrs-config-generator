			    || GLOBAL_PROFILE_CUSTOM) \
		) \
		|| PORT03_INST01_PROFILE_HA_WR \
		|| PORT03_INST01_PROFILE_CUSTOM
	default PORT03_INST01_EXTENSION_NONE if (PORT03_INST01_PROFILE_KEEP_GLOBAL && GLOBAL_PROFILE_PTP) || PORT03_INST01_PROFILE_PTP
